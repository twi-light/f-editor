{
  "manifest_version": 2,
  "name": "f-editor",
  "description": "A WYSIWYG Editor for F-List.",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["https://www.f-list.net/*"],
      "js": ["dist/f-editor.js"]
    }
  ],
  "permissions": [
    "tabs",
    "https://www.f-list.net/*"
  ]
}
